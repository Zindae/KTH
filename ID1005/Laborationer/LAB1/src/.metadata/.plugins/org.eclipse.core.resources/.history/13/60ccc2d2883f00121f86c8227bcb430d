//main
import java.util.*;

public class LAB4_Datum

	public static void main (String[] args)
	{
		Scanner read = new java.util.Scanner (System.in);
		
		System.out.println("Hur gammal är du?");
		int a = read.nextInt();
		
		System.out.println("Vilken månad fyller du år?");
		int b = read.nextInt();
		
		System.out.println("Vilken dag av denna månad fyller du år?");
		int c = read.nextInt();
		
		LAB4_Datum_KLASS2 objekt = new LAB4_Datum_KLASS2(a,b,c);
		//använder värderna inom parantesen i konstruktorn i KLASS2
		
		//LAB4_Datum_KLASS3 objekt2 = new LAB4_Datum_KLASS3("Gabriel", objekt);
		//används i KLASS3 (theName, theDate)
		//varje gång man vill göra ett objekt till en sträng används toString metoden
		//System.out.println(objekt2);
	}
}