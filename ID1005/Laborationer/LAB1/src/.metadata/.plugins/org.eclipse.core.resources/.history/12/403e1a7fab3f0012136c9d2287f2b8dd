import javax.swing.JOptionPane;


public class Konstruktor2
{
	String fuser;
	String fpass;
	
	public Konstruktor2(String user, String pass) //en konstruktor
	{
		fuser = user;
		fpass = pass;
		greet();
	}
	
	private void greet()
	{
		
		if (fpass.equals("vilen") && fuser.equals("gabriel"))
		{
			System.out.println("Välkommen " + fuser + "\nDitt lösenord är: " + fpass);
		}
		else
		{
			System.out.println("Du skrev fel användarnamn eller lösenord, stick!");
		}
	
	}

}
