
public class Up13 
{
	public static void main (String[] args)
	{
	    int    k = 10;
	StringBuilder    s = new StringBuilder ("aaaa");
	
	change (k, s);
	
	System.out.println (k);
	System.out.println (s);
	}
	public static change (int k, StringBuilder sb)
	{
	    k++;
	sb = new StringBuilder ("bbbb");
	sb.append ("xxxx");
	
	//funkar inte 
	}

}
