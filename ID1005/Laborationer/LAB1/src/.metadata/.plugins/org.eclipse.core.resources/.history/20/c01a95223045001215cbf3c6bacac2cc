
public class Up3 
{
	public static void main (String[] args)
	{
   int    k = 10;
	int[]    v = {10, 3, 8, 15, 2};
	
	change (k, v);
	
	   System.out.println (k);
	System.out.println (java.util.Arrays.toString (v));
	}
	
	public static void change (int k, int[] u)
	{
	    k++;
	// u = new int[u.length];
	for (int i = 0; i < u.length; i++)
	        u[i] = (int) (10 * Math.random ()) + 1;
	}

}
